issn = {0829-318X},
journal = {Australian Journal of Botany},
keywords = {allocation,allometry,assimilate,assimilats,conductivity,conduit,conduit tapering,conifers,douglas-fir,drought tolerance,frequency,hydraulic conduc-,leaf-to-sapwood ratio,model,mod{\`{e}}le,partitioning,phloem sieve cells,picea abies,ponderosa pine,puits,sapwood water,sink,source,tivity,xylem trac-,—},
