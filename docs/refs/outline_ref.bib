author = {K{\"{o}}nig, Patrizia and Tautenhahn, Susanne and Cornelissen, J. Hans C. and Kattge, Jens and B{\"{o}}nisch, Gerhard and R{\"{o}}mermann, Christine},
doi = {10.1111/geb.12696},
file = {:Users/deirdreloughnan/Downloads/K-nig{\_}et{\_}al-2018-Global{\_}Ecology{\_}and{\_}Biogeography.pdf:pdf},
