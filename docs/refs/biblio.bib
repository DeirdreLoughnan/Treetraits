	journal = {Ecology Letters},
	keywords = {Chilling,Forcing,Leaf-out,Phenology,Photoperiod,Temperature},
	number = {8},
	pages = {1016--1025},
	pmid = {24943497},
	title = {{Common garden comparison of the leaf-out phenology of woody species from different native climates, combined with herbarium records, forecasts long-term change}},
