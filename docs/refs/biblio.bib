	issn = {0269-8463, 1365-2435},
	journal = {Functional Ecology},
	language = {en},
	month = dec,
	number = {6},
	pages = {1192--1201},
	title = {A multi‐trait approach reveals the structure and the relative importance of intra‐ vs. interspecific variability in plant traits},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/j.1365-2435.2010.01727.x},
