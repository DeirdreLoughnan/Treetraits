	date-added = {2024-01-24 23:47:59 -0800},
	date-modified = {2024-01-24 23:47:59 -0800},
	doi = {10.1111/1365-2745.12074},
	editor = {Gilliam, Frank},
	file = {De Frenne et al. - 2013 - Latitudinal gradients as natural laboratories to i.pdf:/Users/deirdreloughnan/Zotero/storage/EJ8TXVUL/De Frenne et al. - 2013 - Latitudinal gradients as natural laboratories to i.pdf:application/pdf},
