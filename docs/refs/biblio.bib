	isbn = {0028-0836},
	issn = {0028-0836},
	journal = {Nature},
	keywords = {Biodiversity,Biome,Climate,Data reduction,Economic and social effects,Geography,Investments,Leaf economics spectrum,Physiology,Spectrum analysis,Vegetation,climate effect,economics,land use,leaf,nutrient availability,plant leaf},
