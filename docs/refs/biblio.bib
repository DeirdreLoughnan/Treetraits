	doi = {10.1038/nature18608},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Thackeray et al/Nature/Thackeray et al. - 2016 - Phenological sensitivity to climate across taxa and trophic levels.pdf:pdf},
	isbn = {0028-0836},
